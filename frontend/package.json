{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "completion": "ng completion",
    "start": "ng serve -o --port=5555",
    "start:dev": "ng serve --configuration development -o --port=4200",
    "start:prod": "ng serve --configuration production -o --port=4300",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage",
    "lint": "eslint --ext .ts,.html src/",
    "lint:fix": "eslint --ext .ts,.html src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss}\"",
    "clean": "rimraf dist .angular",
    "reinstall": "rimraf node_modules && npm install",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/frontend/stats.json",
    "serve:static": "npx http-server ./dist/frontend -p 8080 -o",
    "update:angular": "ng update @angular/cli @angular/core",
    "outdated": "npm outdated",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "author": "sebastian marin valencia, juan david avila",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.2.10",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@sweetalert2/ngx-sweetalert2": "^14.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "jspdf": "^3.0.1",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.22.4",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@iconify/json": "^2.2.376",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jasmine": "~5.1.0",
    "daisyui": "^5.0.50",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss-animated": "^2.0.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.40.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}